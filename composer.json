{
    "name": "palepurple/nmap",
    "description": "nmap is a PHP wrapper for Nmap, a free security scanner for network exploration.",
    "license": "MIT",
    "authors": [
        {
            "name": "William DURAND",
            "email": "william.durand1@gmail.com"
        },
        {
            "name": "David Goodwin",
            "email": "david+nmap-php@codepoets.co.uk"
        }
    ],
    "require": {
        "php": ">= 7.0",
        "symfony/process": "~2.0|~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "vimeo/psalm": "*",
        "jakub-onderka/php-parallel-lint":"*",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-0": { "Nmap": "src/" }
    },
    "scripts": {
        "lint": "vendor/bin/parallel-lint src",
        "psalm": "vendor/bin/psalm --show-info=false",
        "phpunit": "vendor/bin/phpunit",
        "build" : [
            "@lint",
            "@psalm",
            "@phpunit"
        ]
    }

}
